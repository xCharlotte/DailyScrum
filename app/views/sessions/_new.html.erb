<div class="login">
	<div class="container">
		<div class="form">
			
			<%= form_for(:session, url: login_path) do |f| %>
        <b>Gebruikersnaam</b>
			  <br><%= f.email_field :email %> 
        <br><b>Wachtwoord</b>
			  <br><%= f.password_field :password %> 
			  <br><%= f.submit "LOG IN", class: "btn-submit" %>
			<% end %>
		
    </div>
	</div>
  <div>
    <%= link_to "Registreren", 'signup' %>
  </div>
</div>